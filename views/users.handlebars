         <table class="table table-hover">
                <thead>
                    <tr>
                    <th scope="col">#</th>
                    <th scope="col">User ID</th>
                    <th scope="col">Nickname</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                    <tr>
                        <th scope="row">{{@index}}</th>
                        <td>{{this.user_id}}</td>
                        <td>{{this.user_nickname}}</td>
                    </tr>
                    {{/each}}
                </tbody>
        </table>